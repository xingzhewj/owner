{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true,
            "arrowFunctions": true,
            "classes": true,
            "modules": true,
            "defaultParams": true
        },
        "sourceType": "module"
    },
    "parser": "babel-eslint",
    "plugins": [],
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "no-empty": 0,
        "no-unused-vars": 1,
        "no-console": 0,
        "no-const-assign": 2,
        "no-dupe-class-members": 2,
        "no-duplicate-case": 2,
        "no-extra-parens": [2, "functions"],
        "no-self-compare": 2,
        "accessor-pairs": 2,
        "comma-spacing": [2, {
            "before": false,
            "after": true
        }],
        "constructor-super": 2,
        "new-cap": [2, {
            "newIsCap": true,
            "capIsNew": false
        }],
        "new-parens": 2,
        "no-array-constructor": 2,
        "no-class-assign": 2,
        "no-cond-assign": 2,
        "no-mixed-spaces-and-tabs": 0,
        "no-path-concat": 0,
        "no-undef": 0,
        "comma-dangle": [2, "never"],
        "semi": [2, "always"],
        "indent": [ "error", 4 ],
        "camelcase": 2,
        "array-bracket-spacing": [2, "never"],
        "object-curly-spacing": [2, "never"],
        "block-spacing": [2, "never"],
        "quotes": [2, "single"],
        "key-spacing": [2, { "beforeColon": false, "afterColon": true }],
        "no-trailing-spaces": 1,
        "arrow-spacing": 2,
        "spaced-comment": 2
    }
}